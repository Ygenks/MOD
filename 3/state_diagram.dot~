digraph {

layout="circo";

	000 -> 000[label=<&rho;>];
	000 -> 100[label=<1 - &rho;>];
    
	100 -> 101[label="(1-p)*(1-pi1)"];
    100 -> 001[label="p*(1-pi1)"];
    100 -> 100[label="(1-p)*(1-pi2) + (1-p)pi1 + p*pi1"];

    001 -> 000[label="p(1 - pi2)"];
    001 -> 001[label="p*pi2"];
    001 -> 100[label="(1-p)(1-pi2)"];
    001 -> 101[label="(1-p)pi2"];

    101 -> 100[label="p*pi1(1-pi2) + (1-p)*pi1*(1-pi2)"];
    101 -> 111[label="(1-p)*(1-pi1)*pi2"];
    101 -> 101[label="p*pi1*pi2 + (1-p)*pi1*pi2 + (1-p)*(1-p1)*(1-pi2)"];
    101 -> 001[label="p*(1-pi1)*(1-pi2)"];
    101 -> 011[label="p*(1-pi1)*pi2"];
    
    111 -> 011[label="p*(1-pi1)*pi2"];
    111 -> 001[label="p*(1-pi1)*(1-pi2)"];
    111 -> 101[label="(1-p)*pi1*(1-pi2) + p*pi1*(1-pi2)"];
    // 111 -> 110[label="p*pi1*(1-pi2)"]; //Navernoye kosyak
    111 -> 121[label="(1-p)*(1-pi1)*pi2"];
    111 -> 111[label="(1-p)*(1-pi1)*(1-pi2) + (1-p)*pi1*pi2 + p*pi1*pi2"];

    011 -> 011[label="p*pi2"];
    011 -> 111[label="(1-p)*pi2"];
    011 -> 001[label="p*(1-pi2)"];
    011 -> 101[label="(1-p)*(1-pi2)"];

    121 -> 011[label="p*(1-pi1)*(1-pi2)"];
    121 -> 111[label="p*pi1*(1-pi2) + (1-p)*pi1*(1-pi2)"];
    121 -> 121[label="p*(1-pi1)*pi2 + (1-p)*(1-pi1)*(1-pi2) + (1-p)*(1-pi1)*pi2 + (1-p)*pi1*pi2+p*pi1*pi2"];
    

}
